{
    "theme": "MiTema",
    "colors": {
        "editor.background": "#1e1e1e",
        "editor.foreground": "#d4d4d4"
    },
    "tokenColors": [
        {
            "type": "keyword",
            "foreground": "#569cd6",
            "fontStyle": "bold"
        },
        {
            "type": "string",
            "foreground": "#ce9178"
        },
        {
            "type": "comment",
            "foreground": "#6a9955",
            "fontStyle": "italic"
        }
    ],
    "settings": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    }
}